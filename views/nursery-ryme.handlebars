{{#section 'head'}}
<script src="//cdnjs.cloudflare.com/ajax/libs/handlebars.js/1.3.0/handlebars.min.js"></script>
<script id="nurseryRymeTemplate" type="text/x-handlebrs-template">
	Mary had a little <b>\{{animal}}</b>\{{bodyPart}}</b>
	was <b>\{{adjective}}</b> as <b>\{{none}}</b>.
</script>
{{/section}}
{{#section 'jquery'}}
	$(document).ready(function(){
		var nurseryRymeTemplate = handlebars.compile(
			$('nurseryRymeTemplate').html());
});
{{/section}}
<div id="nurseryRyme">Click a button....</div>
<hr>
<button id="btnNurseryRhyme">Generate nursery ryme</button>
<button id="btnNurseryRhymeAjax">Generate nursery rhyme from AJAX</button>